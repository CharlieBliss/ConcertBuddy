<div id="login-form">
  <div class="container">
    <div class="row">
      <div class="col-lg-6">
        <form id='custom-search' action='songkick/custom_search' method='get'>
          <div class="form-group">
            <label>Location</label>
            <input type="location" class="form-control" name=location placeholder='Location'>
          </div>
          <div class="form-group">
            <label>Start Date</label>
            <input id='start' type='date' class="form-control" value='2016-01-01' name=date[min_date] placeholder='Start Date'>
          </div>
          <div class="form-group">
            <label>End Date</label>
            <input id="end" type='date' class="form-control" value='2016-01-01' name=date[max_date] placeholder='End Date'>
          </div>
          <div class="centered">
            <button type="submit" class="btn btn-large btn-success">Submit</button>
          </div>
        </form>
      </div>
      <div class="col-lg-6">
        <form id='artist-search' action='songkick/artist_search' method='get'>
          <input type='text' name=artist placeholder='artist'>
          <input type='submit' value='Search'>
        </form>
        <form id='has-groups' action='songkick/artist_search' method='get'>
          <input type='submit' value='has groups'>
        </form>
      </div>
    </div>
  </div>
</div>

